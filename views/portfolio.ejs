              <a class="nav-link" href="<%= homeURL %>"><i class="fas fa-home"></i> Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" href="<%= portfolioURL %>"><i class="fas fa-book"></i> Portfolio</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="<%= certificationsURL %>"><i class="fas fa-certificate"></i> Certifications</a>
            </li>

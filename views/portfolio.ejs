    <div class="container-fluid">
      <div class="social-icons">
        <span>Find me:</span><a href="#"><i class="fab fa-linkedin fa-2x" title="LinkedIn"></i></a><a href="#" title="Send me an email"><i class="fas fa-envelope fa-2x"></i></a>
      </div>
      <p>This page has been created by me! @<%= footerTimestamp %></p>
    </div>
